<div class="game-grid">
  <% games.forEach((game) => { %>
  <div class="game-card">
    <img
      src="<%= game.image_path %>"
      alt="<%= game.game_name %>"
      width="300"
      height="300"
    />
    <h3><%= game.game_name %></h3>
    <p><%= game.game_description %></p>
    <a href="/category/edit/<%= game.game_id %>">Edit</a>
  </div>
  <% }); %>
</div>
